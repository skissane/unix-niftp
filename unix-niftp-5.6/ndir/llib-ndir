/* ARGSUSED */
DIR *opendir(name) char *name; { return 0; }
/* ARGSUSED */
struct direct *readdir(dirp) DIR *dirp; {  return 0; }
/* ARGSUSED */
void seekdir(dirp, loc) DIR *dirp; long loc; {;}
/* ARGSUSED */
long telldir(dirp) DIR *dirp; { return( 0 ); }
